# EditorConfig http://EditorConfig.org

# Root file EditorConfig
root = true

[*.md]
trim_trailing_whitespace = false

[*.js]
trim_trailing_whitespace = true

[*.ts]
trim_trailing_whitespace = true

[*.tsx]
trim_trailing_whitespace = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
insert_final_newline = true
max_line_length = 180
